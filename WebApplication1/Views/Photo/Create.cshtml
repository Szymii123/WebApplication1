@model Photo

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Dodaj zdjęcie</h2>
<form asp-action="Create" method="post">
    <div class="mb-3">
        <label asp-for="Author"> Mazwa Autora</label>
        <input asp-for="Author"/>
        <span class="text-danger" asp-validation-for="Author"></span>
       
    </div>
    <div class="mb-3">
        <label class="form-label" asp-for="Hour"> Godzina</label>
        <input class="form-control" asp-for="Hour"/>
        <span class="text-danger" asp-validation-for="Hour"></span>
    </div>
    
    <div class="mb-3">
        <label class="form-label" asp-for="Camera"> Kamera</label>
        <input class="form-control" asp-for="Camera"/>
        <span class="text-danger" asp-validation-for="Camera"></span>
    </div>
    
    <div class="mb-3">
        <label class="form-label" asp-for="Data"> Data</label>
        <input class="form-control" asp-for="Data"/>
        <span class="text-danger" asp-validation-for="Data"></span>
    </div>

    <div class="mb-3">
        <label class="form-label" asp-for="Description"> Opis</label>
        <input class="form-control" asp-for="Description"/>
        <span class="text-danger" asp-validation-for="Description"></span>
    </div>
    
    <div class="mb-3">
        <label class="form-label" asp-for="Format"> Format</label>
        <input class="form-control" asp-for="Format"/>
        <span class="text-danger" asp-validation-for="Format"></span>
    </div>
    
    <div class="mb-3">
        <label class="form-label" asp-for="Id"> Id</label>
        <input class="form-control" asp-for="Id"/>
        <span class="text-danger" asp-validation-for="Id"></span>
    </div>
    
    <div class="mb-3">
        <label class="form-label" asp-for="Resolution"> Rozdzielczość</label>
        <input class="form-control" asp-for="Resolution"/>
        <span class="text-danger" asp-validation-for="Resolution"></span>
    </div>
    
    <div class=" mb-3">
        
        <button class="btn btn-primary" type="submit">Zapisz</button>
    </div>
    
    
    
    
    
    
    
    
</form>
